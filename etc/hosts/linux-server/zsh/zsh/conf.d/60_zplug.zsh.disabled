#
# Zplug configuration
#

source ~/.zsh/plugin/init.zsh

zplug 'zsh-users/zsh-autosuggestions'
zplug 'zsh-users/zsh-syntax-highlighting'
zplug 'zsh-users/zsh-completions'
zplug 'junegunn/fzf'
zplug 'zsh-users/zaw'
zplug 'ascii-soup/zsh-url-highlighter'

if ! zplug check --verbose; then
    printf "Install [y/N]: "
    if read -q; then
        echo; zplug install
    fi
fi

zplug load --verbose
zplug load

