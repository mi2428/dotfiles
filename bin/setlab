#!/bin/bash
if ip link show enp0s20u1 2> /dev/null 1> /dev/null; then
    sudo ip link set enp0s20u1 down
    sudo ip link set enp0s20u1 address 78:31:c1:c0:d7:b8
    sudo ip link set enp0s20u1 up
fi

if ip link show enp0s20u2 2> /dev/null 1> /dev/null; then
    sudo ip link set enp0s20u2 down
    sudo ip link set enp0s20u2 address 78:31:c1:c0:d7:b8
    sudo ip link set enp0s20u2 up
fi
